option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "techbackend.settings"
    PYTHONPATH: "/var/app/current:$PYTHONPATH"



container_commands:
  01_makemigrations:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py makemigrations --noinput"
    leader_only: true
  02_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate --noinput"
    leader_only: true
  04_superuser:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py awscreatesuperuser"
    leader_only: true

  
    